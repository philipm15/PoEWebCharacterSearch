{
  "name": "PoE Web Character Search",
  "version": "1.0",
  "description": "Extension to sort characters by League, Ascendancy and Name",
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://www.pathofexile.com/account/view-profile/*/characters"
      ],
      "js": ["poe.js"]
    }
  ],
  "browser_action": {
    "default_icon": "Albino_Rhoa_Feather.png"
  },
  "manifest_version": 2
}
